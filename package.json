    "postcss": "^8.2.8",
    "react": "^17.0.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-select": "^4.3.0",
    "recharts": "^2.0.9",
    "sweetalert2": "^10.15.7",
    "web-vitals": "^1.0.1"
